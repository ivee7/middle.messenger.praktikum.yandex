{
  "name": "web_messenger",
  "version": "1.0.0",
  "description": "The Web Messenger with minimum of connected libraries",
  "scripts": {
    "dev": "parcel src/index.html --open --no-cache --port 3000",
    "start": "parcel build src/index.html && node ./server.ts",
    "build": "parcel build src/index.html",
    "lint": "eslint ./",
    "stylelint": "npx stylelint '**/*.css'"
  },
  "keywords": [
    "JS",
    "NodeJS",
    "Parcel",
    "Express",
    "PostCSS",
    "ESlint",
    "Web",
    "TypeScript",
    "messenger"
  ],
  "author": "ivees7@yandex.ru",
  "license": "ISC",
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@anikin/parcel-transformer-handlebars-precompile": "1.0.1",
    "@types/node": "18.11.16",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "eslint": "8.29.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.26.0",
    "nanoid": "4.0.0",
    "parcel": "2.8.0",
    "postcss-import": "15.0.1",
    "postcss-nested": "6.0.0",
    "postcss-sassy-mixins": "2.1.0",
    "postcss-simple-vars": "7.0.1",
    "stylelint": "14.16.0",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-scss": "4.3.0",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "express": "4.18.2"
  }
