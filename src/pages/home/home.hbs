                {{{ Link linkType='black' text='404' onClick=goToNotFoundError }}}
            </li>
            <li class="index__link">
                {{{ Link linkType='black' text='Выбрать чат' onClick=goToMain }}}
            </li>
            <li class="index__link">
                {{{ Link linkType='black' text='Чат' onClick=goToMainChat }}}
            </li>
            <li class="index__link">
                {{{ Link linkType='black' text='Меню' onClick=goToShowProfile }}}
            </li>
            <li class="index__link">
                {{{ Link linkType='black' text='Профиль' onClick=goToProfile }}}
            </li>
            <li class="index__link">
                {{{ Link linkType='black' text='Редактировать профиль' onClick=goToEditProfile }}}
