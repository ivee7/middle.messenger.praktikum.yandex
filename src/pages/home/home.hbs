            <li class="index__link">
                {{{ Link linkType='black' text='Авторизация' onClick=goToAuth }}}
            </li>
            <li class="index__link">
                {{{ Link linkType='black' text='Регистрация' onClick=goToRegistr }}}
            </li>
            <li class="index__link">
                {{{ Link linkType='black' text='505' onClick=goToServerError }}}
            </li>
